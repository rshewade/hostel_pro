    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "20.19.27",
    "@types/react": "19.2.7",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.4.0",
